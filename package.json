{
  "name": "ssv-web",
  "description": "Webapp to interact with SSV",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf --glob **/node_modules **/dist **/.next **/.tsbuildinfo",
    "build": "next build",
    "build:stage": "dotenv -e .env.stage -- next build",
    "docker-build": "SKIP_ENV_VALIDATION=true next build",
    "dev": "next dev --turbopack",
    "dev:stage": "dotenv -e .env.stage -- next dev --turbopack",
    "dev:prod": "dotenv -e .env.production -- next dev --turbopack",
    "start": "next start",
    "start:stage": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check",
    "shadcn": "pnpm dlx shadcn@latest",
    "prepare": "husky"
  },
